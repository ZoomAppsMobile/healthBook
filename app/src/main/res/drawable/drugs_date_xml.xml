<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:top="10dp" android:left="10dp" android:bottom="50dp" android:right="50dp">
        <shape android:shape="rectangle">
            <size android:width="3dp"
                android:height="3dp"/>
            <solid android:color="#00000000"/>
        </shape>
    </item>
    <item android:top="4dp" android:left="52dp" android:bottom="52dp" android:right="4dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid
                android:color="#4D67AC"/>
        </shape>
    </item>
</layer-list>
